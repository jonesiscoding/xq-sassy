{
    "name": "jonesiscoding/xq-sassy",
    "description": "PHP Drivers for Dart Sass, SassC, PHP Sass, and scssphp, designed to work with Assetic.",
    "keywords": [
        "sass",
        "scss",
        "php",
        "assetic",
        "pleasing",
        "sassc"
    ],
    "license": "MIT",
    "homepage": "https://www.github.com/jonesiscoding/xq-sassy",
    "version": "2.2.0",
    "authors": [
        {
            "name": "Aaron M Jones",
            "homepage": "https://www.jonesiscoding.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "XQ\\Drivers\\": "src"
        }
    },
    "require": {
        "symfony/process": "^3.4|^4.4|^5.4"
    },
    "replace": {
        "exactquery/xq-sassy": "^1.0"
    },
    "suggest": {
        "scssphp/scssphp": "Used as a backup to Dart Sass or sassc to compile SCSS.  Will function more slowly than binary solutions."
    }
}
